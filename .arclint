{
  "exclude": [],
  "linters": {
    "chmod": {
      "type": "chmod"
    },
    "filename": {
      "type": "filename"
    },
    "generated": {
      "type": "generated"
    },
    "json": {
      "type": "json",
       "include": [
         "(^\\.arcconfig$)",
         "(^\\.arclint$)",
         "(\\.json$)"
       ]
    },
    "merge-conflict": {
      "type": "merge-conflict"
    },
    "nolint": {
      "type": "nolint"
    },
    "phutil-library": {
      "type": "phutil-library",
      "include": "(^\\.php$)"
    },
    "phutil-xhpast": {
      "type": "phutil-xhpast"
    },
    "spelling": {
      "type": "spelling"
    },
    "text": {
      "type": "text"
    },
    "text-without-length": {
      "type": "text",
      "severity": {
        "3": "disabled"
      }
    },
    "xhpast": {
      "type": "xhpast",
      "include": "(\\.php$)",
      "severity": {
        "16": "advice",
        "34": "error"
      },
      "xhpast.php-version": "5.3.0",
      "xhpast.php-version.windows": "5.3.0"
    }
  }
}
